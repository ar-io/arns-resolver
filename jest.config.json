{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "extensionsToTreatAsEsm": [".ts"],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "transform": {
    "^.+\\.m?[tj]sx?$": [
      "ts-jest",
      {
        "useESM": true
      }
    ]
  },
  "moduleFileExtensions": ["ts", "js", "json", "node"]
}
